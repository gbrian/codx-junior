# VNC 
DISPLAY=:1
DISPLAY_SHARED=:2
DISPLAY_WIDTH=1920
DISPLAY_HEIGHT=1080
VNC_NO_PASSWORD=1
# API virtual env
API_VENV=/tmp/.venv_codx_junior_api
# Browser virtual env
BROWSER_VENV=/tmp/.venv_codx_junior_browser
# codx-junior API port
API_PORT=9984
# codx-junior WEB port
WEB_PORT=9983
# codx-junior code-server port
CODER_PORT=9909
# codx-junior NOVNC port
NOVNC_PORT=9986
# Filebrowser port
FILEBROWSER_PORT=9987
# Browser-use port
BROWSER_PORT=9988

DEBIAN_FRONTEND=noninteractive
# Locales
LANG=en_US.UTF-8  
LANGUAGE=en_US:en  
LC_ALL=en_US.UTF-8

# Serving client from API
CODX_SUPERVISOR_LOG_FOLDER=/var/log/codx-junior-supervisor

STATIC_FOLDER=${CODX_JUNIOR_PATH}/client/dist


